.item_normal{
    BORDER-LEFT: #000000 1px solid;
    BORDER-RIGHT: #000000 1px solid;
    background-color:#ffffff;
    line-height:20px;
    overflow:hidden;
}
.itemBg{
    BORDER-LEFT: #000000 1px solid;
    BORDER-RIGHT: #000000 1px solid;
    cursor:default;
    line-height:20px;
    background-color:#ffffff;
}
.item_high{
    BORDER-LEFT: #000000 1px solid;
    BORDER-RIGHT: #000000 1px solid;
    background-color:#0256b8;
    line-height:20px;
    cursor:default;
    color:#666666;
}
.item_button{
    BORDER-LEFT: #000000 1px solid;
    BORDER-RIGHT: #000000 1px solid;
    BORDER-BOTTOM: #000000 1px solid;
    line-height:20px;
    text-align:right;
    background-color:#ffffff;
    cursor:hand;
}
.suggest_hidden{display:none;}