<script type="text/javascript">
var speed=80;
var colee2=document.getElementById("colee2");
var colee1=document.getElementById("colee1");
var colee=document.getElementById("colee");
var notice_element=colee1.getElementsByTagName("li");
if(notice_element.length>5){
	colee2.innerHTML=colee1.innerHTML; //克隆colee1为colee2
	function Marquee1(){
		//当滚动至colee1与colee2交界时
		if(colee2.offsetTop-colee1.offsetTop-colee.scrollTop<=0){
			colee.scrollTop-=colee1.offsetHeight+160; //colee跳到最顶端
		}else{
			colee.scrollTop++;
		}
	}
	var MyMar1=setInterval(Marquee1,speed)//设置定时器
	//鼠标移上时清除定时器达到滚动停止的目的
	colee.onmouseover=function() {clearInterval(MyMar1)}
	//鼠标移开时重设定时器
	colee.onmouseout=function(){MyMar1=setInterval(Marquee1,speed)}
}


var coleediv2=document.getElementById("coleediv2");
var coleediv1=document.getElementById("coleediv1");
var coleediv=document.getElementById("coleediv");
var notice_element=coleediv1.getElementsByTagName("li");
if(notice_element.length>5){
	coleediv2.innerHTML=coleediv1.innerHTML; //克隆coleediv1为coleediv2
	function Marquee2(){
		//当滚动至coleediv1与coleediv2交界时
		if(coleediv2.offsetTop-coleediv1.offsetTop-coleediv.scrollTop<=0){
			coleediv.scrollTop-=coleediv1.offsetHeight+160; //coleediv跳到最顶端
		}else{
			coleediv.scrollTop++;
		}
	}
	var MyMar2=setInterval(Marquee2,speed)//设置定时器
	//鼠标移上时清除定时器达到滚动停止的目的
	coleediv.onmouseover=function() {clearInterval(MyMar2)}
	//鼠标移开时重设定时器
	coleediv.onmouseout=function(){MyMar2=setInterval(Marquee2,speed)}
}
