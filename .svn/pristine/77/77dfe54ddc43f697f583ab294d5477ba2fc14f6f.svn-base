<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
	<head>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<title>常见故障维护</title>
		<link rel="icon" href="favicon.ico" type="image/x-icon" />
		<meta http-equiv="pragma" content="no-cache">
		<meta http-equiv="cache-control" content="no-cache">
		<meta http-equiv="expires" content="0">
		<#include "/admin/common/include.html" />
		
<#if !(cjgz.ID)??>
	<#assign isAdd = true />
<#else>
	<#assign isAdd = false />
</#if>
	</head>
	<body class="input" onload="showMsg('${errMsg}')">
		<div class="ctrttop cy143w">
			<#if isAdd>添加故障<#else>编辑故障</#if>
		</div>
		<div class="body">
			<form id="validateForm"  action="<#if isAdd>${base}/manage/base/cjgz/save<#else>${base}/manage/base/cjgz/update</#if>" method="post">
				<input type="hidden" name="cjgz.ID" value="${(cjgz.ID)!}" />
				<table class="inputTable">
				<tr>
						<th>
							设备类型
						</th>
						<td>
						<select name="cjgz.SBLX" id="sblx" class="labelselect formText {required: true}" >
							<option value="">请选择</option>
							<#list statics["cn.com.jandar.plugin.DicPlugin"].jkzidianList.get("SBLX") as sblx>
								<#if ((cjgz.SBLX)!) == sblx.value>
									<option value="${sblx.value}" selected="selected">
								<#else >
									<option value="${sblx.value}" >
								</#if>
									${sblx.value}
								</option>
							</#list>
						</select>
						<label class="requireField">*</label>
						</td>
					</tr>
				 <tr>
						<th>
							故障说明:
						</th>
				
					    <td>
						  <textarea class="formTextarea {required: true}" name="cjgz.GZSM" style="width:500px">${(cjgz.GZSM)!}</textarea>
						  <label class="requireField">*</label>
					   </td>
				</tr>
				
				</table>
				<div class="buttonArea">
					<input type="submit" class="formButton" value="确  定" hidefocus="true" />
					&nbsp;&nbsp;&nbsp;&nbsp;
					<input type="button" class="formButton" onclick="window.history.back(); return false;" value="返  回" hidefocus="true" />
				</div>
			</form>
		</div>
	</body>
</html>
