<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
	<head>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<title>厂商查看</title>
		<link rel="icon" href="favicon.ico" type="image/x-icon" />
		<meta http-equiv="pragma" content="no-cache">
		<meta http-equiv="cache-control" content="no-cache">
		<meta http-equiv="expires" content="0">
		<#include "/admin/common/include.html" />

	</head>
	<body class="input" onload="showMsg('${errMsg}')">
		<div class="ctrttop cy143w">
			厂商查看
		</div>
		<div class="body">
			<form id="validateForm"  action="<#if isAdd>${base}/manage/base/factory/save<#else>${base}/manage/base/factory/update</#if>" method="post">
				<table class="inputTable">
				<tr>
						<th>
							厂商名称:
						</th>
						<td>
						    <!--  <input type="text" name="factory.FNAME" value="${(factory.FNAME)!}" readonly="readonly" class="formText {required: true}"/>
						    <label class="requireField">*</label>
								-->
							<label>${(factory.FNAME)!}</label>
						</td>
					</tr>
				 <tr>
						<th>
							厂商简称:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.FSNAME" readonly="readonly" class="formText {required: true}" value="${(factory.FSNAME)!}" />
							<label class="requireField">*</label>
						-->
							<label>${(factory.FSNAME)!}</label>
						</td>
				 </tr>
				  <tr>
						<th>
							联系人:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.LXR" readonly="readonly" class="formText {required: true}" value="${(factory.LXR)!}" />
							<label class="requireField">*</label>
						-->
							<label>${(factory.LXR)!}</label>
						</td>
				 </tr>
				 <tr>
						<th>
							联系电话:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.LXDH" readonly="readonly" class="formText {required: true, phone: true}" value="${(factory.LXDH)!}" />
							<label class="requireField">*</label>
						-->
							<label>${(factory.LXDH)!}</label>
							
						</td>
				 </tr>
				 <tr>
						<th>
							手机:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.PHONE" readonly="readonly" class="formText {mobile: true} " value="${(factory.PHONE)!}" />
							-->
							<label>${(factory.PHONE)!}</label>
						</td>
				 </tr>
				 <tr>
						<th>
							传真:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.FOX" readonly="readonly" class="formText " value="${(factory.FOX)!}" />
							-->
							<label>${(factory.FOX)!}</label>
						</td>
				 </tr>
				<tr>
					<th>
						邮箱:
					</th>
					<td>
					<!-- 
						<input type="text" name="factory.EMAIL" readonly="readonly" class="formText {required: true, email: true}" value="${(factory.EMAIL)!}" />
						<label class="requireField">*</label>
						-->
						<label>${(factory.EMAIL)!}</label>
					</td>
				</tr>
				<tr>
					<th>
						地 址:
					</th>
					<td>
					<!-- 
						<input type="text" name="factory.ADDR" readonly="readonly" class="formText " style="width:500px" value="${(factory.ADDR)!}" />
						-->
						<label>${(factory.ADDR)!}</label>
					</td>
				</tr>
				<tr>
						<th>
							邮政编码:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.YZBM" readonly="readonly" class="formText {zipCode: true} " value="${(factory.YZBM)!}" />
							-->
							<label>${(factory.YZBM)!}</label>
						</td>
				 </tr>
				<tr>
						<th>
							开户行:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.KHH" readonly="readonly" class="formText {required: true}" value="${(factory.KHH)!}" />
							<label class="requireField">*</label>
							-->
							<label>${(factory.KHH)!}</label>
						</td>
				 </tr>
				<tr>
						<th>
							帐号:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.ZHANGHAO" readonly="readonly" class="formText {required: true}" value="${(factory.ZHANGHAO)!}" />
							<label class="requireField">*</label>
							-->
							<label>${(factory.ZHANGHAO)!}</label>
						</td>
				 </tr>
				 <tr>
						<th>
							户名:
						</th>
						<td>
						<!-- 
							<input type="text" name="factory.HUMING" readonly="readonly" class="formText {required: true}" value="${(factory.HUMING)!}" />
							<label class="requireField">*</label>
							-->
							<label>${(factory.HUMING)!}</label>
						</td>
				 </tr>
				 <tr>
						<th>
							备注:
						</th>
						<td>
						<!-- 
							<textarea class="formTextarea" name="factory.BZ" readonly="readonly" style="width:500px">${(factory.BZ)!}</textarea>
						-->	
							<label>${(factory.BZ)!}</label>
						</td>
				 </tr>
				
				
				
				</table>
				<div class="buttonArea">
					
					<input type="button" class="formButton" onclick="window.history.back(); return false;" value="返  回" hidefocus="true" />
				</div>
			</form>
		</div>
	</body>
</html>
